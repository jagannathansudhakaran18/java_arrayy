import java.util.ArrayList;

public class SumArrayList {
    public static void main(String[] args) {
        ArrayList<Integer> list = new ArrayList<>();
        int sum = 0;

        for (int i = 1; i <= 10; i++) {
            list.add(i);
        }

        for (int x : list) {
            sum += x;
        }

        System.out.println("Sum = " + sum);
    }
}
